}

.dhx_cal_event_clear.dhx_cal_event_line_start.dhx_cal_event_line_end {
    text-overflow: ellipsis !important;
    white-space: nowrap !important;
}

.dhx_cal_event_clear.dhx_cal_event_line_start.dhx_cal_event_line_end.eventStatusNew {
    background: #A7E2D6;
    color: #1B615E;
}

.dhx_cal_event_clear.dhx_cal_event_line_start.dhx_cal_event_line_end.eventStatusAssigned {
    background: #FFCE36;
