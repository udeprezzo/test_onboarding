html, body {
    margin: 0px;
    padding: 0px;
    height: 100%;
    overflow: hidden;
    font-family: 'Salesforce Sans',Arial,sans-serif;
}

.dhx_title {
    font-size: 11px !important;
    padding-top: 5px !important;
    white-space: pre-wrap !important;
    word-wrap: break-word !important;
    text-overflow: ellipsis !important;
}

.dhx_body {
    padding-bottom: 3px !important;
    line-height: 15px !important;
    text-align: center !important;
    font-size: 11px !important;
    white-space: pre-wrap;
    overflow: hidden;
    word-wrap: break-word !important;
}

.dhx_cal_event .dhx_footer {
    background: none !important;

}
.dhx_cal_container {
    position: absolute;
}
.eventStatusNew .dhx_title,
.eventStatusNew .dhx_body {
    background: #A7E2D6;
    color: #1B615E;
}

.eventStatusAssigned .dhx_title,
.eventStatusAssigned .dhx_body {
    background: #FFCE36;
    color: #644D06;
}

.eventStatusDispatched .dhx_body,
.eventStatusDispatched .dhx_title {
    background: #92D9F7;
    color: #125977;
}

.eventStatusTravel .dhx_body,
.eventStatusTravel .dhx_title {
    background: #D495F4;
    color: #60206C;
}

.eventStatusOnSite .dhx_body,
.eventStatusOnSite .dhx_title {
    background: #D8C68B;
    color: #352404;
}

.eventStatusCompleted .dhx_body,
.eventStatusCompleted .dhx_title {
    background: #A3DF6D;
    color: #1C6708;
}

.eventStatusIncomplete .dhx_body,
.eventStatusIncomplete .dhx_title {
    background: #F27D7D;
    color: #3A0707;
}

.eventCustomStatus .dhx_body,
.eventCustomStatus .dhx_title {
    background: #B7C9EA;
    color: #2D3542;
}

.NA_Break .dhx_body {
    background: -webkit-repeating-linear-gradient(45deg, #5e698f, #5e698f 12.5px, #535C7D 12.5px, #535C7D 25px) !important;
    background: repeating-linear-gradient(45deg, #5e698f, #5e698f 12.5px, #535C7D 12.5px, #535C7D 25px) !important;
}

.NA_Break .dhx_title {
    background: rgb(83, 92, 125) !important;
    padding: 3px 0 2px 0 !important;
    text-overflow: ellipsis !important;
    white-space: nowrap !important;
}

.NA_Break .dhx_body a,
.NA_Break .dhx_title a {
    color: #fff;
}

.NA_Absence_NoTravel .dhx_title {
    color: #3A0707;
    background: #D7DFE7 !important;
    padding: 2px 0 3px 0 !important;
    text-overflow: ellipsis !important;
    white-space: nowrap !important;
}

.NA_Absence_Monthly,
.NA_Absence_NoTravel .dhx_body {
    background: -webkit-repeating-linear-gradient(45deg, rgba(215, 223, 231, 1), rgba(215, 223, 231, 1) 12.5px, rgba(206, 214, 221, 1) 12.5px, rgba(206, 214, 221, 1) 25px) !important;
    background: repeating-linear-gradient(45deg, rgba(215, 223, 231, 1), rgba(215, 223, 231, 1) 12.5px, rgba(206, 214, 221, 1) 12.5px, rgba(206, 214, 221, 1) 25px) !important;
    color: #3A0707;
}
.dhx_cal_event_line.eventStatusNew {
    background: #A7E2D6;
    color: #1B615E;
}

.dhx_cal_event_line.eventStatusAssigned {
