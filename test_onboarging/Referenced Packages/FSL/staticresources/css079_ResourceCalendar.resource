.dhx_cal_event_clear.dhx_cal_event_line_start.dhx_cal_event_line_end.eventStatusTravel {
    background: #D495F4;
    color: #60206C;
}

.dhx_cal_event_clear.dhx_cal_event_line_start.dhx_cal_event_line_end.eventStatusOnSite {
    background: #D8C68B;
    color: #352404;
}

.dhx_cal_event_clear.dhx_cal_event_line_start.dhx_cal_event_line_end.eventStatusCompleted {
    background: #A3DF6D;
    color: #1C6708;
}

.dhx_cal_event_clear.dhx_cal_event_line_start.dhx_cal_event_line_end.eventStatusIncomplete {
    background: #F27D7D;
    color: #3A0707;
}


.dhx_cal_event_line.dhx_cal_event_line_start.NA_Break,.dhx_cal_event_line.dhx_cal_event_line_end.NA_Break{
     background: -webkit-repeating-linear-gradient(45deg, #5e698f, #5e698f 12.5px, #535C7D 12.5px, #535C7D 25px) !important;
     background: repeating-linear-gradient(45deg, #5e698f, #5e698f 12.5px, #535C7D 12.5px, #535C7D 25px) !important;
      color: #fff;
}

#businessHours {
    position: relative;
    top: 20px;
    right: 140px;
    z-index: 10;
    float: right;
}

#yearGradient {
    position: fixed;
    top: 18px;
    right: 106px;
    z-index: 10;
    height: 30px;
    display: none;
}


#yearGradient span {
    display: inline-block;
    width: 10px;
    height: 10px;
    margin: 6px 8px 0 12px;
}

.LegendHigh { background: rgba(125, 195, 125, 1); }
.LegendMed { background: rgba(125, 195, 125, 0.5); }
.LegendLow { background: rgba(125, 195, 125, 0.25); }
.LegendNA { background: #ccc; }

#Apply {
    display: inline-block;
    color: #fff;
    font-size: 12px;
    position: relative;
    top: -6px;
    cursor: pointer;
    border-radius: 0 3px 3px 0;
    border: 1px solid #D8DDE6;
    padding: 6px;
    right: 6px;
}

#Apply:hover {
    background-color: #f4f6f9 !important;
}

#Apply svg {
    width: 18px;
    height: 18px;
    fill: #16325c;
}

.dhx_cal_today_button {
    width: 53px !important;
    right: 49px !important;
    text-overflow: ellipsis !important;
    white-space: pre-wrap !important;
    border-radius: 0 !important;
    border-color: #D8DDE6 !important;
}

.dhx_tooltip_line .dhx_event_icon {
    cursor: auto !important;

}

.dhx_cal_prev_button,
.dhx_cal_next_button {
    width: 34px !important;
    border-color: #D8DDE6 !important;
}

.dhx_cal_prev_button {
    right: 103px !important;
}

.lightboxLabel {
    color: #676767;
    font-size: 12px;
    margin-bottom: 3px;
    display: block;
}

.LightboxPopUp {
    position: absolute;
    top: 51px;
    left: 239px;
    z-index: 10001;
    background-color: white;
    width: 236px;
    box-shadow: 10px 10px 5px -8px rgba(0, 0, 0, 0.75);
    border-radius: 3px;
    border: 1px solid #CCC;

}

.LightboxHeader {
    background: #fff;
    padding: 8px;
    border-bottom: 2px solid #D8DDE6;
}

.LightboxName {
    color: #55698D;
    font-size: 19px;
}

.LightboxClose {
    right: 9px;
    position: absolute;
    top: 8px;
    color: #55698D;
    border-radius: 50%;
    padding: 3px 5px;
    background: #fff;
    cursor: pointer;
}

.LighboxContent {
    padding: 15px;
}

.fieldRow {
    margin-bottom: 10px;
    display: inline-table;
}

#leftside {
    display: inline-table;
}

#leftside .fieldRow input {
    border-radius: 3px;
    border: 1px solid #CCC;
    padding: 5px;
    width: 26px;
}

#leftside .fieldRow select {
    border-radius: 3px;
    border: 1px solid #CCC;
    padding: 5px;
    width: 70px;
    margin-right: 22px;
}

.LightboxButtons {
    border-radius: 0 0 3px 3px;
    padding: 0 10px 10px 10px;
}

.Btn {
    background: #0877bd;
    padding: 8px;
    width: 100px;
    font-size: 14px;
    color: #fff;
    text-align: center;
    margin: 0 auto;
    border-radius: 3px;
    cursor: pointer;
    /* margin-bottom: 10px;*/
    margin-top: 13px; /**/
}

.printIcon {
    position: absolute;
    color: #B4AEAC !important;
    background: #fff;
    cursor: pointer !important;
    top: 10px;
    margin-left: 254px !important;
    margin-top: 5px !important;
}

#rightside {
    display: inline-block;
    width: 162px;
    margin-left: 42px;
    position: relative;
    top: -2px;
}

.dhx_year_tooltip {
    width: 450px !important;
}

.tooltipLine {
    color: #222;
    margin: 4px 0;
}

.tooltipHR {
    height: 1px;
    width: 100%;
    background: #D5D5D5;
    margin: 12px 0;
}

.tooltipCell {
    display: inline-block;
    width: 105px;
}

.tooltipTitle {
    font-weight: bold;
}

.expertTooltip {
    border: 1px solid #CECECE;
    box-shadow: 2px 2px 5px #CCC !important;
    border-radius: 3px;
    color: #1c1c1c;
    max-width: 500px;
    padding: 0 !important;
}

.tooltipBody {
    padding: 8px 10px;
}

.tooltipBreakIcon {
    width: 17px !important;
    height: 17px !important;
}

.tooltipNAIcon {
    width: 15px !important;
    height: 15px !important;
}
.tooltipMultiDayIcon {
    width: 14px !important;
    height: 14px !important;
}

.naIcon {
    width: 12px !important;
    height: 12px !important;
    float: left;
    position: relative;
    margin: 1px 0 0 2px;
}

.breakIcon {
    width: 14px !important;
    height: 14px !important;
    float: left;
    position: relative;
    margin: -2px 0 0 2px;
}
.multiDay{
    width: 12px !important;
    height: 12px !important;
    float: left;
    position: relative;
    margin-top: -4px;
    margin-right: -2px;
    margin-bottom: 0px;
    margin-left: 2px;
}
.multiDayYearTt{
    width: 10px !important;
    height: 10px !important;
    float: left;
    position: relative;
    margin-top: 4px;
    margin-right: 0px;
    margin-bottom: 0px;
    margin-left: -2px;
}
