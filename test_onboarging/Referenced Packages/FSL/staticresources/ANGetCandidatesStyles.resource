.sf1 #CandidatesContainer,.sf1 #ScheduleButton{right:9px!important;left:9px!important}#AN-NoCandidates,#AN-Results,#ScheduleButton{text-align:center}#CandidatesContainer{overflow-y:auto}.AN-candidate,.AN-candidates-slot{overflow:hidden;border-bottom:1px solid #d8dde6;cursor:pointer}.AN-with-button{height:calc(100vh - 132px)!important}.AN-candidate{padding:5px 10px 7px}.AN-candidate:hover{background:#EBF0F5}.AN-candidate img{width:32px;height:32px;border-radius:30px;float:left;margin:11px 12px 0 0}.AN-candidates-name{position:relative;top:5px}.AN-candidates-next-date{color:#778AAB;font-size:.75em;margin:10px 25px 3px 41px;display:block}.AN-candidates-slot{padding:12px 15px;display:block;background:#f7f9fb;font-size:.85em;position:relative}.AN-candidates-slot:hover{background:#EBEEF1}.AN-candidates-slot input{margin-right:15px}.AN-candidates-grade{border-radius:2px;padding:3px 6px;position:absolute;right:12px;top:10px}.AN-grade-excellent{background:#6BD25B;color:#fff}.AN-grade-good{background:#ffd33d}.AN-grade-ok{background:#ff6c45;color:#fff}#ScheduleButton{background:#0070d2;color:#FFF;cursor:pointer;padding:15px 0;font-size:18px;position:fixed;bottom:0;left:0;right:0}#ScheduleButton:hover{background:#0467BD}#BigCheck{display:block;margin:50px auto;fill:#6BD25B}.AN-ResultText{padding:0 30px;line-height:2em}#AN-UndoButton{color:#54698D;cursor:pointer;border:1px solid #D8DDE6;border-radius:3px;margin:50px 20px 10px;padding:15px}#AN-UndoButton:hover{background:#f4f6f9;color:#0070d2}.AN-current-slot-padding:hover,.AN-selected-resource{background:#EAF4FF}#AN-UndoButton+div{margin:0 20px 10px;font-size:.8em;line-height:1.6em;color:#8799B5}.AN-has-good-grades{fill:#EFBA05;width:20px;height:20px;float:right;position:relative;top:10px;right:-2px}.AN-current-slot{fill:#6BD25B;width:20px;height:20px;position:absolute;left:15px;top:11px}.AN-current-slot-padding{padding-left:48px;background:#EAF4FF;cursor:default}.AN-selected-resource-name{color:#0070D2}.AN-selected-resource:hov