.CloseLightbox {
    top: -39px;
    cursor: pointer;
    fill: #FFF !important;
    float: right;
    position: absolute;
    right: 0;
    width: 30px !important;
    height: 30px !important;
}
.ExtendedForm {
    float: right;
    font-size: 18px;
    margin-top: 5px;
}

.ExtendedForm a,
.ExtendedForm a:visited
.ExtendedForm a:active {
      color: #0070D2;
      text-decoration: none;
}


.ExtendedForm .slds-icon {
    width: 20px;
    height:20px;
    fill:#0070d2;
}

.light-box-header {
    margin-bottom: 22px !important;
}

.slds-theme--alert-texture {
    background-image: linear-gradient(45deg,rgba(0,0,0,.025) 25%,transparent 25%,transparent 50%,rgba(0,0,0,.025) 50%,rgba(0,0,0,.025) 75%,transparent 75%,transparent);
    background-size: 64px 64px;
}

.slds-theme--error {
    color: #fff;
    background-color: #c23934 !important;
    font-family: 'Salesforce Sans', Arial, sans-serif !important;
    font-weight: 200;
    text-align: center;
    padding: 20px;
    font-size: 16px;
    border-top-right-radius: 2px;
    border-top-left-radius: 2px;
}

.notCapacityBasedContent {
    padding: 15px;
    text-align: center;
}

