    <validationRules>
        <fullName>Absence_Color_HEX_Format</fullName>
        <active>true</active>
        <errorConditionFormula>(!ISBLANK(Gantt_Color__c)) &amp;&amp; (NOT(REGEX(Gantt_Color__c, &quot;#[a-fA-F0-9]{6}&quot;)))</errorConditionFormula>
        <errorDisplayField>Gantt_Color__c</errorDisplayField>
        <errorMessage>Gantt color must be in hexadecimal format: (for example: #ffffff)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NoAddressOnBreaks</fullName>
        <active>true</active>
        <errorConditionFormula>AND( $RecordType.Name =&apos;Break&apos; , NOT(ISBLANK(Street) &amp;&amp; ISBLANK(City) &amp;&amp; ISBLANK(Country) &amp;&amp; ISBLANK(State) &amp;&amp; ISBLANK(PostalCode)  ))</errorConditionFormula>
        <errorMessage>Cannot add address to resource absence with record type &apos;Break&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>startShouldPrecedeEnd</fullName>
        <active>true</active>
        <errorConditionFormula>End &lt;=  Start</errorConditionFormula>
        <errorDisplayField>Start</errorDisplayField>
        <errorMessage>The Start Time must be earlier than the End Time.</errorMessage>
    </validationRules>
